<div id="lwbar-header">
  <div class="limit-toolbar">
    <h1 class="logo-toolbar">Central do Cliente</h1>
    <% if @logoutUrl :%>
      <a href="<%= @logoutUrl %>" accesskey="q" data-method="delete" class="log-out" title="Para sair, pressione CONTROL + ALT + Q">Sair</a>
    <% end %>
    <% if @userName :%>
      <span class="user-name"><strong><%= @userName %></strong></span>
    <% end %>
  </div>
</div>
